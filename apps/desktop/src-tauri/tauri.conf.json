{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Meet Scribe",
  "version": "0.0.1",
  "identifier": "com.srprasanna.meet-scribe",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Meet Scribe",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
